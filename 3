import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);

        System.out.println("enter your age: ");
        int num = input.nextInt();

        if (num > 18){
            System.out.println("так ви можете піти на голосування ");
        }
        else {
            System.out.println("нажаль ви не можетие пройти голосування дочекайтесь повноліття!!!");

        }
    }
}
